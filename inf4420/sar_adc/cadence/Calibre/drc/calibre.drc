//=========
#IFDEF N80
  M3A = M3Wide_N80first NOT CB_NON_CUP
#ENDIF
M31 = M3Wide_first NOT CB_NON_CUP
M32 = M3Wide_1.5 NOT CB_NON_CUP
M33 = M3Wide_4.5 NOT CB_NON_CUP

VARIABLE M3_A_2pre ((M3_A_2 + (M3_S_1 * M3_S_1 * 3.142))/(2 * M3_S_1))*((M3_A_2 + (M3_S_1 * M3_S_1 * 3.142))/(2 * M3_S_1))/3.141

M3HO = HOLES M3 INNER < M3_A_2pre

M3HOC = M3HO NOT M3

M3AS3 = AREA M3 > M3_S_3_L*M3_W_1  // filter small metal for M3.S.3
M3AS4 = AREA M3AS3 > M3_S_4_L*M3_W_1  // filter small metal for M3.S.4

M3.W.1 { @ Width >= 0.14 
  INT M3 < M3_W_1 ABUT < 90 SINGULAR REGION
}
M3.W.2 { @ Width of 45-degree bent Mx Please make sure the vertex of 45 degree pattern is on 5nm grid (refer to the guideline, G.5gU, in section 3.6) >= 0.19 
  INT M3_EDGE_45 < M3_W_2 ABUT < 90 REGION
}
#IFDEF RECOMMENDED_NONSHRINKABLE_RULE
  #IFDEF N80
    M3.W.2.R { @ Width of 45-degree bent Mx 0.205  
      INT M3_EDGE_45 < M3_W_2R ABUT < 90 REGION
    }
  #ENDIF
#ENDIF

M3.W.3 { @ Maximum width <= 12.00 
  SIZE M3 BY M3_W_3 /2 UNDEROVER TRUNCATE M3_W_3 /2
}
M3.S.1 { @ Space >= 0.14 
  EXT M3 < M3_S_1 ABUT < 90 SINGULAR REGION
}
M3.S.2 { @ Space [at least one metal line width > 0.21 um (W1) and the parallel metal run length > 0.52 um (L1)] >= 0.19 
  X = EXT M31 M3 < M3_S_2 OPPOSITE REGION MEASURE ALL
  Y = X NOT M3
  ENCLOSE RECTANGLE Y GRID M3_S_2_L+GRID ORTHOGONAL ONLY
}
#IFDEF N80
  M3.S.2.1 { @ Space [at least one metal line width > 0.21um(W1) and the parallel metal run length > 0.8um(L1)] >= 0.15um
  X = EXT M3A M3 < M3_S_2_1 OPPOSITE REGION MEASURE ALL
  Y = X NOT M3
  ENCLOSE RECTANGLE Y GRID M3_S_2_1_L+GRID ORTHOGONAL ONLY
  }
#ENDIF

M3.S.3 { @ Space [at least one metal line width > 1.5 um (W2) and the parallel metal run length > 1.5 um (L2)] >= 0.50 
  X = EXT M32 M3AS3 < M3_S_3 OPPOSITE REGION MEASURE ALL
  Y = X NOT M3
  ENCLOSE RECTANGLE Y GRID M3_S_3_L+GRID
}
M3.S.4 { @ Space [at least one metal line width > 4.5 um (W3) and the parallel metal run length > 4.5 um (L3)] >= 1.50 
  X = EXT M33 M3AS4 < M3_S_4 OPPOSITE REGION MEASURE ALL
  Y = X NOT M3
  ENCLOSE RECTANGLE Y GRID M3_S_4_L+GRID
}
M3.S.5 { @ Space to 45-degree bent Mx >= 0.19 
  X = EXPAND EDGE M3_EDGE_45 OUTSIDE BY M3_S_5
  X AND M3
}
#IFDEF RECOMMENDED_NONSHRINKABLE_RULE
  #IFDEF N80
    M3.S.5.R { @ Mx space to 45-degree bent Mx 0.205  
      X = EXPAND EDGE M3_EDGE_45 OUTSIDE BY M3_S_5R
      X AND M3
    }
  #ENDIF
#ENDIF
M3.EN.1 { @ Enclosure of VIAx-1 >= 0.005 
  ENC VIA2 M3 < M3_EN_1 ABUT < 90 SINGULAR REGION
  VIA2 NOT M3
}
M3.EN.2_M3.EN.3 { @ Enclosure of VIAx-1 [at least two opposite sides] >= 0.05 
  X = RECTANGLE ENCLOSURE VIA2 M3 ABUT < 90 SINGULAR GOOD M3_EN_1 M3_EN_2 OPPOSITE M3_EN_1 M3_EN_2 OPPOSITE
  ENC X M3 < M3_EN_3 ABUT < 90 SINGULAR REGION
}

M3.A.1 { @ Area >= 0.07 
  AREA M3 < M3_A_1
}
M3.A.2 { @ Enclosed area >= 0.20 
  AREA M3HOC < M3_A_2
}


