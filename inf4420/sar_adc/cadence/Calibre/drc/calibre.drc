      [ AREA(M7_AND_DM7EXCL_HIGH)/AREA(CHIP_AND_DM7EXCL_HIGH) ]
    ERR2 = DENSITY M7_NOT_EXC_HIGH CHIP_NOT_M7_EXC_HIGH > Mx_DN_1H WINDOW Mx_DN_W2 STEP 50 BACKUP INSIDE OF LAYER CHIP 
