ESD_PMOS_DRAIN_pre4  = ESD_PSD NOT ESD_PMOS_SOURCE_pre4 
ESD_PMOS_normal = (ESD_PMOS INTERACT ESD_PMOS_SOURCE_pre4) INTERACT ESD_PMOS_DRAIN_pre4
ESD_PMOS_special = ESD_PMOS NOT ESD_PMOS_normal 
